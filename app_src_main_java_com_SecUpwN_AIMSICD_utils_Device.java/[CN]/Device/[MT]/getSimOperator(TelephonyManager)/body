{
  try {
    if (tm.getSimState() == TelephonyManager.SIM_STATE_READY) {
      mSimOperator=(tm.getSimOperator() != null) ? tm.getSimOperator() : "N/A";
    }
 else {
      mSimOperator="N/A";
    }
  }
 catch (  Exception e) {
    mSimOperator="N/A";
    Log.e(TAG,mTAG + ":getSimOperator " + e);
  }
  if (mSimOperator.isEmpty()) {
    mSimOperator="N/A";
  }
  return mSimOperator;
}
