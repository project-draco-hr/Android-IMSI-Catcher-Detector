{
  b.setOnClickListener(new View.OnClickListener(){
    @Override public void onClick(    View view){
      Intent intent=new Intent(Intent.ACTION_VIEW,Uri.parse(getResources().getString(link)));
      startActivity(intent);
    }
  }
