{
  final String mChmod;
  final Context mInstance=ctx;
  if (RootTools.checkUtil("chmod"))   mChmod="chmod";
 else {
    if (RootTools.checkUtil("busybox") && RootTools.hasUtil("chmod","busybox"))     mChmod="busybox chmod";
 else     if (RootTools.checkUtil("toolbox") && RootTools.hasUtil("chmod","toolbox"))     mChmod="toolbox chmod";
 else     mChmod="";
  }
}
