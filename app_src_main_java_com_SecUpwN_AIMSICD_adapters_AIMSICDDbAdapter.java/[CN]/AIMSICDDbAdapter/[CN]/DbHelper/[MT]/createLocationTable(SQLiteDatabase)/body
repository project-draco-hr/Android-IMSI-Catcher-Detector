{
  String LOC_DATABASE_CREATE="CREATE TABLE " + LOCATION_TABLE + " ("+ COLUMN_ID+ " INTEGER PRIMARY KEY AUTOINCREMENT, "+ "Lac INTEGER, CellID INTEGER, "+ "Net VARCHAR, "+ "Lat VARCHAR, "+ "Lng VARCHAR, "+ "Signal INTEGER, "+ "Connection VARCHAR, "+ "Timestamp TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP);";
  database.execSQL(LOC_DATABASE_CREATE);
}
