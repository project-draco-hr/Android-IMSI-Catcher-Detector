{
  String OPENCELLID_DATABASE_CREATE="CREATE TABLE " + OPENCELLID_TABLE + " ("+ COLUMN_ID+ " INTEGER PRIMARY KEY AUTOINCREMENT, "+ "Lat VARCHAR, "+ "Lng VARCHAR, "+ "Mcc INTEGER, "+ "Mnc INTEGER, "+ "Lac INTEGER, "+ "CellID INTEGER, "+ "AvgSigStr INTEGER, "+ "avg_range INTEGER, "+ "Samples INTEGER, "+ "isGPSexact INTEGER, "+ "Type TEXT, "+ "Timestamp TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP"+ ");";
