{
  this.cid=cid;
  this.lac=lac;
  mcc=Integer.MAX_VALUE;
  mnc=Integer.MAX_VALUE;
  if (dbm) {
    this.dbm=signal;
  }
 else {
    this.rssi=signal;
  }
  this.psc=psc;
  this.netType=netType;
  timingAdvance=Integer.MAX_VALUE;
  sid=Integer.MAX_VALUE;
  lon=0.0;
  lat=0.0;
  speed=0.0;
  accuracy=0.0;
  bearing=0.0;
  this.timestamp=SystemClock.currentThreadTimeMillis();
}
