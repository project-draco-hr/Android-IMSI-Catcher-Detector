{
  if (mAtCommand.getText() != null) {
    String command=mAtCommand.getText().toString();
    if (command.toUpperCase().indexOf("AT") == 0) {
      Log.i("AIMSICD","AT Command Detected");
      executeAT();
    }
 else {
      Log.i("AIMSICD","Terminal Command Detected");
      executeCommand();
    }
  }
}
