{
  if (mAtCommand.getText() != null) {
    mAtResponse.setText("");
    String cmdSetup="cat " + mRilDevice + " &";
    new ExecuteAtCommand().execute(cmdSetup);
    String atCommand=mAtCommand.getText().toString();
    new ExecuteAtCommand().execute("echo -e " + atCommand + "\r > "+ mRilDevice);
  }
}
