{
  int responseType;
  Parcel p;
  if (DBG) {
    Log.i(TAG,"received " + length + " bytes: "+ HexDump.toHexString(data,pos,length));
  }
  p=Parcel.obtain();
  try {
    p.unmarshall(data,pos,length);
    p.setDataPosition(0);
    responseType=p.readInt();
switch (responseType) {
case RESPONSE_UNSOLICITED:
      Log.i(TAG,"Unsolicited response ");
    break;
case RESPONSE_SOLICITED:
  processSolicited(p);
break;
default :
Log.i(TAG,"Invalid response type: " + responseType);
break;
}
}
  finally {
p.recycle();
}
}
