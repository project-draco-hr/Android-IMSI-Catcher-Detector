{
  if (params[0] == null || params[0].trim().equals("")) {
    return FAILURE;
  }
  String stdout=null;
  if (mMountSystem) {
    Helpers.getMount("rw");
  }
  try {
    for (    String param : params) {
      if (param != null && !param.trim().equals("")) {
        stdout=CMDProcessor.runSuCommand(param).getStdout();
      }
 else {
        return FAILURE;
      }
    }
  }
  finally {
    if (mMountSystem) {
      Helpers.getMount("ro");
    }
  }
  return stdout;
}
