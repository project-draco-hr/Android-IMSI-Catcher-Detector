{
  try {
    if (Shell.shell == null) {
      RootShell.log("Starting Shell!");
      Shell.shell=new Shell("/system/bin/sh",ShellType.NORMAL,ShellContext.NORMAL,timeout);
    }
 else {
      RootShell.log("Using Existing Shell!");
    }
    return Shell.shell;
  }
 catch (  RootDeniedException e) {
    throw new IOException();
  }
}
