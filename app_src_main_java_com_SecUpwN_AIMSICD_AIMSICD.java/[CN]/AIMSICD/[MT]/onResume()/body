{
  super.onResume();
  invalidateOptionsMenu();
  if (!mBound) {
    Intent intent=new Intent(this,AimsicdService.class);
    bindService(intent,mConnection,Context.BIND_AUTO_CREATE);
  }
 else {
    if (mAimsicdService.isTrackingCell()) {
      mAimsicdService.checkLocationServices();
    }
  }
}
