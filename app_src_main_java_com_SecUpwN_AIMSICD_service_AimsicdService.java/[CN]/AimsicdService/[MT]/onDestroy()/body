{
  super.onDestroy();
  mCellTracker.stop();
  mLocationTracker.stop();
  mAccelerometerMonitor.stop();
  mRilExecutor.stop();
  try {
    if (smsdetector.getSmsDetectionState()) {
      smsdetector.stopSmsDetection();
    }
  }
 catch (  Exception ee) {
    Log.e(TAG,"Error stopping sms detection\n" + ee.toString());
  }
  Log.i(TAG,"Service destroyed.");
}
