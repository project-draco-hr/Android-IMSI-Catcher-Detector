{
  if (mSimOperator.isEmpty() || force) {
    try {
      if (tm.getSimState() == TelephonyManager.SIM_STATE_READY) {
        mSimOperator=(tm.getSimOperator() != null) ? tm.getSimOperator() : "N/A";
      }
 else {
        mSimOperator="N/A";
      }
    }
 catch (    Exception e) {
      mSimOperator="N/A";
      Log.e(TAG,"getSimOperator " + e);
    }
  }
  return mSimOperator;
}
